    <div id="canvas-wrap">
        <canvas id="myCanvas">
        </canvas>
        <div id="map" style="height:800px;">
        </div>
    </div>


    <script>
        //draw circle
        var c = document.getElementById("myCanvas");
        var ctx = c.getContext("2d");
        ctx.beginPath();
        ctx.arc(150, 80, 55, 0, 4 * Math.PI);
        ctx.stroke();

        //initialize map
        function myMap() {
            var locations = [];
            $.get("https://localhost:44304/Stores", function (data) {
                var uluru = { lat: 31.957122, lng: 34.970975 };
                // The map, centered at Uluru
                var map = new google.maps.Map(
                    document.getElementById('map'), { zoom: 11, center: uluru });

                // The marker, positioned at Uluru
                $.each(data, function (i, item) {
                    let location = { lat: item.lat, lng: item.lng };
                    var marker = new google.maps.Marker({ position: location, map: map });
                });
            });

            $("#myCanvas").click(function () {
                $("#myCanvas").hide();
            });

        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyByfBxIl9_Kz12_97dMtFdVDlbn7zk7dts&callback=myMap"></script>


</div>
